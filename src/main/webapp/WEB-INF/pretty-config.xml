<pretty-config
	xmlns="http://ocpsoft.com/prettyfaces/3.3.3"
	xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
	xsi:schemaLocation="
		http://ocpsoft.com/prettyfaces/3.3.3
		http://ocpsoft.com/xml/ns/prettyfaces/ocpsoft-pretty-faces-3.3.3.xsd">

	<url-mapping id="index">
		<pattern value="/" />
		<view-id value="/app/list" />
	</url-mapping>

	<url-mapping id="entry-point">
		<pattern value="/start" />
		<view-id value="/app/list" />
	</url-mapping>
	
	<url-mapping id="errorPage">
		<pattern value="/error" />
		<view-id value="/error.html" />
	</url-mapping>
</pretty-config>